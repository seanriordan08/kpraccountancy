	<meta name="viewport" content="width=device-width, initial-scale=1.0">

  <%= csrf_meta_tags %>
</head>

<body class="bg-two-tone">
	<div class="container">

		<p class="notice"><%= notice %></p>
		<p class="alert"><%= alert %></p>

		<%= render partial: "shared/nav_bar.html.erb" %>
